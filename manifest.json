{
  "manifest_version": 2,
  "name": "rodc",
  "description": "run on download completed",
  "version": "0.0.1",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_specific_settings": {
    "gecko": {
      "id": "rodc@example.net",
      "strict_min_version": "50.0"
    }
  },
  "permissions": [
    "nativeMessaging",
    "downloads"
  ]
}
